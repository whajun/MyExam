#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int plus(int x, int y) {
	return x + y;
}
int minus(int x, int y) {
	return x - y;
}
int multiple(int x, int y) {
	return x * y;
}
double divide(int x, int y) {
	return (double)x / y;
}

int main(void)
{
	int a, b;
	while (1) {
		scanf("%d %d", &a, &b);
		printf("plus : %d\n", plus(a, b));
		printf("minus : %d\n", minus(a, b));
		printf("multiple : %d\n", multiple(a, b));
		printf("divide : %f\n", divide(a, b));
	}
	printf("\n");
	return 0;
}
