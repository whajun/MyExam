#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main(void)
{
	
	
	while (1) {
		printf("십구단을 외자, 십구단을 외자, 몇 단?\n");
	int i, dan;
	scanf("%d", &dan);
	printf("%d단!\n", dan);
	if (dan <= 19 && dan >= 2) {
		for (i = 1; i <= 19; i++) {
			printf("%d * %d = %d\n", dan, i, dan * i);
		}
	}
	else {
		printf("2~19 사이의 숫자를 입력하세요\n");
	}
}
	return 0;
}
