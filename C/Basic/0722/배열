#include <stdio.h>

int main() {
    int N, v;
    int arr[100];
    int count = 0;

    scanf("%d", &N); // 정수 개수 입력
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]); // N개의 정수 입력
    }

    scanf("%d", &v); // 찾을 숫자 입력

    for (int i = 0; i < N; i++) {
        if (arr[i] == v) {
            count++; // v와 같은 수가 있으면 count 증가
        }
    }
